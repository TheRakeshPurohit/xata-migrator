{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "DOM"],
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": true
  }
}
